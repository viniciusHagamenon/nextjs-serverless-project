{
  "extends": ["next/core-web-vitals", "prettier", "plugin:cypress/recommended"],
  "plugins": [
    "cypress"
  ],
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always-and-inside-groups",
        "groups": [["external", "builtin"], "internal", "parent", "sibling", "index"]
      }
    ]
  }
}
